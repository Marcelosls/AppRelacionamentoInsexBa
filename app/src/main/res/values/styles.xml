<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundedImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>


    </style>
    <style name="EstiloCheckBox">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:buttonTint">#20568E</item> <!-- Cor do check -->
        <item name="android:padding">8dp</item>
    </style>

    <style name="ConfigTransparentTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Theme.SeuApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customizações -->
        <item name="colorPrimary">@color/outros</item>
        <item name="colorPrimaryVariant">#0C263B</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <item name="colorSecondary">@color/outros</item>
    </style>



</resources>